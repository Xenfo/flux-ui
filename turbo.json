{
  "$schema": "https://turborepo.com/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "lint": {
      "dependsOn": [],
      "outputs": []
    },
    "test": {
      "dependsOn": [],
      "outputs": []
    },
    "build": {
      "dependsOn": ["lint", "test", "^build"],
      "outputs": ["lib/**", ".next/**"]
    },
    "publish": {
      "dependsOn": ["build"],
      "cache": false
    },
    "dev": {
      "cache": false
    }
  }
}
