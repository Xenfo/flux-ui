{
  "name": "flux-ui",
  "private": true,
  "author": "Samuel Corsi-House <chouse.samuel@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xenfo/flux-ui.git"
  },
  "workspaces": [
    "docs/",
    "packages/*"
  ],
  "scripts": {
    "postinstall": "is-ci || husky install",
    "commit": "cz",
    "new": "plop",
    "build": "FORCE_COLOR=1 turbo run build --parallel",
    "dev": "FORCE_COLOR=1 turbo run dev --parallel",
    "lint": "FORCE_COLOR=1 turbo run lint --parallel",
    "publish": "FORCE_COLOR=1 turbo run publish --parallel",
    "format": "prettier -w --ignore-path .prettierignore .",
    "test": "jest",
    "test:ci": "jest -i --ci",
    "upgrade": "node scripts/upgrade.mjs",
    "release": "node scripts/release.mjs",
    "sb:start": "start-storybook -p 6006",
    "sb:build": "build-storybook",
    "chromatic": "chromatic --build-script-name=sb:build",
    "swc-node": "INIT=$INIT_CWD node -r @swc/register -r ./scripts/utils/prerun.ts"
  },
  "resolutions": {
    "@storybook/react/webpack": "^5.70.0",
    "ansi-regex": "^3.0.1",
    "glob-parent": "^6.0.2",
    "minimist": "^1.2.6",
    "trim": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@commitlint/cz-commitlint": "^17.0.0",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@storybook/addon-a11y": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/builder-webpack5": "^6.5.6",
    "@storybook/manager-webpack5": "^6.5.6",
    "@storybook/react": "^6.5.6",
    "@swc/core": "^1.2.196",
    "@swc/helpers": "^0.4.0",
    "@swc/jest": "^0.2.21",
    "@swc/register": "^0.1.10",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/eslint": "^8.4.2",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.0.0",
    "@types/node": "^17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@xenfo/prettier-plugin-sort-imports": "^3.2.1",
    "chalk": "^5.0.1",
    "chromatic": "^6.5.6",
    "commitizen": "^4.2.4",
    "eslint": "^8.17.0",
    "eslint-config-marine": "^9.4.1",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-storybook": "^0.6.0",
    "execa": "^6.1.0",
    "fs-extra": "^10.1.0",
    "globby": "^13.1.1",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "lint-staged": "^13.0.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.6",
    "npm-check-updates": "^16.0.0",
    "plop": "^3.1.0",
    "prettier": "^2.6.2",
    "rollup": "^2.75.5",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-ts": "^3.0.1",
    "storybook-dark-mode": "^1.1.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "turbo": "^1.2.16",
    "typescript": "^4.7.3"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "packageManager": "yarn@3.2.3"
}
