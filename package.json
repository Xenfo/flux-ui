{
  "name": "flux-ui",
  "private": true,
  "author": "Samuel Corsi-House <chouse.samuel@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xenfo/flux-ui.git"
  },
  "workspaces": [
    "docs/",
    "packages/*"
  ],
  "scripts": {
    "postinstall": "is-ci || husky install",
    "commit": "cz",
    "new": "plop",
    "build": "FORCE_COLOR=1 turbo run build --parallel",
    "dev": "FORCE_COLOR=1 turbo run dev --parallel",
    "lint": "FORCE_COLOR=1 turbo run lint --parallel",
    "publish": "FORCE_COLOR=1 turbo run publish --parallel",
    "format": "prettier -w --ignore-path .prettierignore .",
    "test": "jest",
    "test:ci": "jest -i --ci",
    "upgrade": "node scripts/upgrade.mjs",
    "release": "node scripts/release.mjs",
    "sb:start": "start-storybook -p 6006",
    "sb:build": "build-storybook",
    "chromatic": "chromatic --build-script-name=sb:build",
    "swc-node": "INIT=$INIT_CWD node -r @swc/register -r ./scripts/utils/prerun.ts"
  },
  "resolutions": {
    "@storybook/react/webpack": "^5.70.0",
    "ansi-regex": "^6.0.0",
    "glob-parent": "^5.1.2",
    "minimist": "^1.2.6",
    "trim": "^0.0.3",
    "rollup-plugin-ts@2.0.5": "patch:rollup-plugin-ts@npm:2.0.5#.yarn/patches/rollup-plugin-ts-npm-2.0.5-9d9fec7bf1.patch"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@commitlint/cz-commitlint": "^16.2.4",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@storybook/addon-a11y": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-webpack5": "^6.4.22",
    "@storybook/manager-webpack5": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@swc/core": "^1.2.181",
    "@swc/helpers": "^0.3.13",
    "@swc/jest": "^0.2.21",
    "@swc/register": "^0.1.10",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/eslint": "^8.4.2",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "chalk": "^5.0.1",
    "chromatic": "^6.5.4",
    "commitizen": "^4.2.4",
    "eslint": "^8.15.0",
    "eslint-config-marine": "^9.4.1",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-storybook": "^0.5.11",
    "execa": "^6.1.0",
    "fs-extra": "^10.1.0",
    "globby": "^13.1.1",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "lint-staged": "^12.4.1",
    "lodash": "^4.17.21",
    "minimist": "^1.2.6",
    "npm-check-updates": "^12.5.11",
    "plop": "^3.1.0",
    "prettier": "^2.6.2",
    "rollup": "^2.72.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-ts": "patch:rollup-plugin-ts@^2.0.5#./.yarn/patches/rollup-plugin-ts-npm-2.0.5-9d9fec7bf1.patch",
    "storybook-dark-mode": "^1.1.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "turbo": "^1.2.8",
    "typescript": "^4.6.4"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "packageManager": "yarn@3.2.1"
}
