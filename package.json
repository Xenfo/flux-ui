{
  "name": "flux-ui",
  "private": true,
  "author": "Samuel Corsi-House <chouse.samuel@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xenfo/flux-ui.git"
  },
  "workspaces": [
    "docs/",
    "packages/*"
  ],
  "scripts": {
    "postinstall": "is-ci || husky install",
    "commit": "cz",
    "new": "plop",
    "build": "FORCE_COLOR=1 turbo run build --parallel",
    "dev": "FORCE_COLOR=1 turbo run dev --parallel",
    "lint": "FORCE_COLOR=1 turbo run lint --parallel",
    "publish": "FORCE_COLOR=1 turbo run publish --parallel",
    "format": "prettier -w --ignore-path .prettierignore .",
    "test": "jest",
    "test:ci": "jest -i --ci",
    "upgrade": "node scripts/upgrade.mjs",
    "release": "node scripts/release.mjs",
    "sb:start": "start-storybook -p 6006",
    "sb:build": "build-storybook",
    "chromatic": "chromatic --build-script-name=sb:build",
    "swc-node": "INIT=$INIT_CWD node -r @swc/register -r ./scripts/utils/prerun.ts"
  },
  "resolutions": {
    "@storybook/react/webpack": "^5.70.0",
    "rollup-plugin-ts@2.0.5": "patch:rollup-plugin-ts@npm:2.0.5#.yarn/patches/rollup-plugin-ts-npm-2.0.5-9d9fec7bf1.patch"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/cz-commitlint": "^16.2.3",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@storybook/addon-a11y": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@swc/core": "^1.2.161",
    "@swc/helpers": "^0.3.8",
    "@swc/jest": "^0.2.20",
    "@swc/register": "^0.1.10",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.0.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/eslint": "^8.4.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "chalk": "^5.0.1",
    "chromatic": "^6.5.3",
    "commitizen": "^4.2.4",
    "eslint": "^8.12.0",
    "eslint-config-marine": "^9.4.1",
    "eslint-config-next": "^12.1.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-storybook": "^0.5.7",
    "execa": "^6.1.0",
    "fs-extra": "^10.0.1",
    "globby": "^13.1.1",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "lodash": "^4.17.21",
    "minimist": "^1.2.6",
    "npm-check-updates": "^12.5.4",
    "plop": "^3.0.5",
    "prettier": "^2.6.1",
    "rollup": "^2.70.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-ts": "patch:rollup-plugin-ts@^2.0.5#./.yarn/patches/rollup-plugin-ts-npm-2.0.5-9d9fec7bf1.patch",
    "storybook-dark-mode": "^1.0.9",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "turbo": "^1.1.10",
    "typescript": "^4.6.3"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "packageManager": "yarn@3.2.0"
}
