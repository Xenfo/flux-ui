{
  "extends": [
    "marine/prettier/react",
    "plugin:storybook/recommended",
    "../../.eslintrc"
  ],
  "rules": {
    "react/prop-types": ["off"],
    "react/jsx-filename-extension": ["error", { "extensions": [".tsx"] }]
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "settings": {
    "react": {
      "version": "latest"
    }
  }
}
